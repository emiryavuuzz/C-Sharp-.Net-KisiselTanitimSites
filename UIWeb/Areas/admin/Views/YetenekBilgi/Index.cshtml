<div class="container">
    <a href="/admin/YetenekBilgi/Insert" class="btn btn-primary" style="margin-bottom:15px">Yeni Ekle</a>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>#</td>
                <td>Yetenek Adı</td>
                <td>Tecrübe Yılı</td>
                <td>Bilgi Oranı</td>
                <td>İşlemler</td>
            </tr>

        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.YetenekAdi</td>
                        <td>@item.TecrubeYili</td>
                        <td>@item.BilgiOrani</td>

                        <td>
                            <a href="/admin/YetenekBilgi/update/@item.Id" class="btn btn-primary">Güncelle</a>
                            <a href="/admin/YetenekBilgi/delete/@item.Id" class="btn btn-success">Sil</a>


                        </td>

                    </tr>
            }


        </tbody>

    </table>
</div>